<div class="content-wrapper">
  {{view SwapzPOS.FlashView modelBinding="flash"}}
  <div class="row">
    <div class="col-lg-8">
      <h4>
        <i class="fa fa-group"></i>
        &nbsp;
        Print Sale
      </h4>
    </div>
    <div class="col-lg-4">
      <div class="pull-right">
        <button {{ action 'print'}} class="btn btn-flat">
          <i class="fa fa-print"></i>
          &nbsp;
          Print
        </button>
      </div>
    </div>
  </div>
  <hr>
  <div class="row">
    <div class="col-lg-12">
      {{view Ember.Select content=templates value=template optionValuePath="content" optionLabelPath="content.name" prompt="Default" class="form-control"}}
    </div>
  </div>
  <hr>
  <div class="row">
    <div class="col-lg-12">
      {{#if templatePath }}
        {{view SwapzPOS.IFrameView src=templatePath }}
      {{/if}}
    </div>
  </div>
  <hr>